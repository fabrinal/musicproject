<p id="notice"><%= notice %></p>

<h1>Listing Fans</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>User name</th>
      <th>Password</th>
      <th>City</th>
      <th>Zip code</th>
      <th>User type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fans.each do |fan| %>
      <tr>
        <td><%= fan.first_name %></td>
        <td><%= fan.user_name %></td>
        <td><%= fan.password %></td>
        <td><%= fan.city %></td>
        <td><%= fan.zip_code %></td>
        <td><%= fan.user_type %></td>
        <td><%= link_to 'Show', fan %></td>
        <td><%= link_to 'Edit', edit_fan_path(fan) %></td>
        <td><%= link_to 'Destroy', fan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fan', new_fan_path %>
